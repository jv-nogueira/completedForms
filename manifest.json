{
    "manifest_version": 3,
    "name": "Predefinidor de tabulação",
    "version": "0.1",
    "permissions": ["activeTab"],
    "icons": { 
      "48": "icons/operation.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "modal.js",
          "modalActions.js"
        ],
        "css": [
          "modal.css"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["modal.css"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  